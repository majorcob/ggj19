pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- jacob cazabon 2019

local t,f=true,false

local px,py,pc,pr,pwt

function _init()
	px,py=64,64
	pc=t
	pr=t
	pwt=0
end

function _update()
	if pc then
 	pwt+=1
 	local wth,wtv=f
 	if btn(0)==btn(1) then
 		wth=t
 	elseif btn(0) then
 		px-=1
 		pr=f
 	elseif btn(1) then
 		px+=1
 		pr=t
 	end
 	if btn(2)==btn(3) then
 		wtv=t
 	elseif btn(2) then
 		py-=1
 	elseif btn(3) then
 		py+=1
 	end
 	if (wth and wtv) pwt=0
 end
end

function _draw()
	rectfill(0,0,127,127,1)
	local ps=pwt%20<10 and 1 or 2
	spr(ps,px-4,py,1,1,pr,f)
end
